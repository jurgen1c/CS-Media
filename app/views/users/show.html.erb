<%= @user.avatar %>
<%= @user.name %>
<%= @user.username %>
<br>
<% @user.follows.each do |follow| %>
  <%= follow.name%>
  <%= follow.username%>
  <%= nu_display(current_user, follow)%>
<%end%>
<br>
<% @user.articles.each do |article| %> 
  <%= article.title %>
  <%= article.votes.count %>
  <br>
  <%= article.cover %>
  <hr>
  <%= article.body %>
<%end%>

  
